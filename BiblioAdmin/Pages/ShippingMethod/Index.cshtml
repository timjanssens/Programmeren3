@page
@model BiblioAdmin.Pages.ShippingMethod.IndexModel
@{
}


<h1>Shipping Methods</h1>

<a href="/ShippingMethod/InsertingOne" class="tile">
    <span class="icon-plus"></span>
    <span class="screen-reader-text">Inserting One</span>
</a>

<table class="list">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Naam
            </th>
            <th>
                Beschrijving
            </th>
            <th>
                Prijs
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ShippingMethodList)
        {
        <tr>
            <td>
                <a href="ShippingMethod/ReadingOne/@item.Id">Select</a> |
            </td>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Description
            </td>
            <td>
                @item.Price
            </td>
        </tr>
        }
    </tbody>
</table>